		<othersQuitRoom>
			<Arg>	INT8	</Arg>
		</othersQuitRoom>
		<quitRoomFailed>
			<Arg>	UINT32	</Arg>
		</quitRoomFailed>
		<quitRoomSucceed>
		</quitRoomSucceed>
