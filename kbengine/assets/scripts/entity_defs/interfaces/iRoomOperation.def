			<Arg> UINT8 </Arg>
			<Arg> INT8 </Arg>
		</voteDismissResult>
		<notifyPlayerOnlineStatus>
			<Arg> UINT8 </Arg>
			<Arg> INT8 </Arg>
		</notifyPlayerOnlineStatus>
