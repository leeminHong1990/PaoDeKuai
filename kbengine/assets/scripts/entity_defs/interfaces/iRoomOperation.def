		<voteDismissRoom>
			<Exposed/>
			<Arg>	INT8 </Arg>
		</voteDismissRoom>
		<setIntegral>
			<Arg> UID </Arg>
			<Arg> INT32 </Arg>
		</setIntegral>
		<reqIsAfford>
			<Exposed/>
			<Arg> INT32 </Arg>
		</reqIsAfford>
