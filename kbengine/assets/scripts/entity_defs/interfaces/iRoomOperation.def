			<Arg>	INT8	</Arg>
			<Arg>	INT8	</Arg>
			<Arg>	INT8	</Arg>
		</recvMagicEmotion>
		<recvEmotion>
			<Arg>	INT8	</Arg>
			<Arg>	INT8	</Arg>
		</recvEmotion>
		<recvMsg>
			<Arg>	INT8	</Arg>
			<Arg>	INT8	</Arg>
		</recvMsg>
		<recvVoice>
			<Arg>	INT8	</Arg>
			<Arg>	STRING	</Arg>
		</recvVoice>
		<recvAppVoice>
			<Arg>	INT8	</Arg>
			<Arg>	STRING	</Arg>
			<Arg>	FLOAT </Arg>
		</recvAppVoice>
		<handleReconnect>
			<Arg> REC_ROOM_INFO	 </Arg>
		</handleReconnect>

		<pushRoundRecord>
			<Arg> ROUND_RECORD_DICT </Arg>
