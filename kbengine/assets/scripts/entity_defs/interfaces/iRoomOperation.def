		</confirmOperation>
		<roundEndCallback>
			<Exposed/>
		</roundEndCallback>
		<upLocationInfo>
			<Exposed/>
			<Arg>	UNICODE </Arg>
			<Arg>	STRING </Arg>
			<Arg>	STRING </Arg>
		</upLocationInfo>
